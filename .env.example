# AI Provider 선택 (gemini 또는 claude)
AI_PROVIDER=gemini

# Gemini 설정 (무료) - https://ai.google.dev 에서 API 키 발급
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_MODEL=gemini-2.5-flash

# Claude 설정 (유료) - AI_PROVIDER=claude 로 변경 시 사용
ANTHROPIC_API_KEY=your_anthropic_api_key_here
CLAUDE_MODEL=claude-sonnet-4-20250514

# 공통 설정
MAX_TOKENS=1024
